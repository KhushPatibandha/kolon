fun: main() {
    var a: string = scan();
    var b: string = scan("here is some random prompt: ");
    var c: string = scan("here is some random prompt 2", true);
    var d: string = scan("here is some random prompt: ", false);

    if: (a == ""): {
        println(true);
    }
    println(a);
    println(b);
    println(c);
    println(d);
    println(typeOf(a));

    var a: int = toInt(scan("Enter a number", true));
    println(a);
    println(typeOf(a));

    var a: float = toFloat(scan("Enter a number", true));
    println(a);
    println(typeOf(a));
}
