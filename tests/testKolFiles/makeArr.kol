// Take array size input from user and add that number of elements to an array
fun: main() {
    var arr: int[] = [];
    var n: int = toInt(scanln("Enter the length of the array: "));
    for: (var i: int = 0; i < n; i++): {
        var num: int = toInt(scanln("Enter element number " + toString(i + 1) + ": "));
        push(arr, num);
        println(arr);
    }
}
