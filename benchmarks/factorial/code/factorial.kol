fun: main() {
    var result: int;
    var p: int;
    for: (var i: int = 0; i < 1000000; i++): {
        result = factorial(20);
        p = i;
    }
    println(result);
    println(p);
}

fun: factorial(n: int): (int) {
    if: (n == 0 || n == 1): {
        return: 1;
    }
    return: n * factorial(n - 1);
}
